# Arch
# CONFIG_ARCH_CROW is not set

# Fingerprint
CONFIG_FINGERPRINT_GOODIX_FOD=m
CONFIG_INPUT_FINGERPRINT=y

# IR
CONFIG_IR_SPI=m
CONFIG_LIRC=y
CONFIG_RC_CORE=y
CONFIG_RC_DECODERS=y
CONFIG_RC_DEVICES=y

# MTD
CONFIG_MTD=m
CONFIG_MTD_BLOCK=m
CONFIG_MTD_BLOCK2MTD=m
CONFIG_MTD_OOPS=m

# Power supply
CONFIG_WIRELESS_REVERSE_CLOSE=y

# Product
CONFIG_TARGET_PRODUCT_VERMEER=y

# Regulator
CONFIG_REGULATOR_WL2866D=m
CONFIG_REGULATOR_WL2868C=m

# Thermal
CONFIG_MI_THERMAL_INTERFACE=m
CONFIG_MI_THERMAL_MULTI_CHARGE=y

# Xiaomi HWID
CONFIG_MI_HARDWARE_ID=m

# ZRAM
CONFIG_ZRAM_WRITEBACK=y
